alias gst='git status'
alias gco='git checkout'
alias gc='git commit -m'
alias ga='git add'
alias gp='git push'
alias gpl='git pull'
alias gb='git branch -a'
alias gr='git reset --soft HEAD~1'
alias gt='git log --since="yesterday" --pretty="%s" --reverse'
alias gtl='git log --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit'
